<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="#e6000000"/>
            <corners android:radius="33.3dp"/>
            <stroke android:width="1px" android:color="#80000000"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#e6000000"/>
            <corners android:radius="33.3dp"/>
            <stroke android:width="1px" android:color="#80000000"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="#e6000000"/>
            <corners android:radius="33.3dp"/>
            <stroke android:width="1px" android:color="#80000000"/>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="#00000000"/>
            <corners android:radius="33.3dp"/>
            <stroke android:width="1px" android:color="#80000000"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#14000000"/>
            <stroke android:width="1px" android:color="#80000000"/>
            <corners android:radius="33.3dp"/>
        </shape>
    </item>
</selector>
