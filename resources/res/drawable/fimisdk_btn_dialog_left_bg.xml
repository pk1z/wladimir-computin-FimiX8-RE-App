<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#e5e5e5"/>
            <corners android:bottomLeftRadius="4.333dp"/>
            <stroke android:width="0.333dp" android:color="#33000000" android:left="-2dp" android:bottom="-2dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#f2f2f2"/>
            <corners android:bottomLeftRadius="4.333dp"/>
            <stroke android:width="0.333dp" android:color="#33000000" android:left="-2dp" android:bottom="-2dp"/>
        </shape>
    </item>
</selector>
