<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:background="#ffffffff" android:fitsSystemWindows="true" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.fimi.libperson.widget.TitleView android:id="@+id/title_view" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    <EditText android:textSize="13.3dp" android:textColor="#cc000000" android:textColorHint="#33000000" android:layout_gravity="center_vertical" android:id="@+id/et_cs_search" android:background="@drawable/shape_et_country_select_search" android:layout_width="match_parent" android:layout_height="34dp" android:layout_marginLeft="17.6dp" android:layout_marginTop="28.3dp" android:layout_marginRight="17.6dp" android:hint="@string/country_select_search" android:singleLine="true" android:layout_below="@+id/title_view" android:layout_centerHorizontal="true" android:textCursorDrawable="@drawable/shape_login_cursor_color"/>
