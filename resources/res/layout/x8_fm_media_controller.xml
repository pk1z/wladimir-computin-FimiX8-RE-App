<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:id="@+id/activity_general_setting" android:layout_width="match_parent" android:layout_height="match_parent">
    <ImageButton android:id="@+id/prev" android:visibility="gone" style="@style/MediaButton.Previous"/>
    <ImageButton android:id="@+id/rew" android:visibility="gone" style="@style/MediaButton.Rew"/>
    <ImageButton android:id="@+id/ffwd" android:visibility="gone" style="@style/MediaButton.Ffwd"/>
    <ImageButton android:id="@+id/next" android:visibility="gone" style="@style/MediaButton.Next"/>
    <RelativeLayout android:id="@+id/process_play" android:background="#cc1f1f21" android:layout_width="match_parent" android:layout_height="67dp">
        <TextView android:background="#26ffffff" android:layout_width="match_parent" android:layout_height="0.33dp"/>
        <TextView android:textSize="12dp" android:textColor="#ffffffff" android:id="@+id/time_current" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_marginTop="21dp"/>
        <ImageButton android:id="@+id/pause" android:visibility="visible" android:layout_width="40dp" android:layout_height="40dp" android:layout_centerVertical="true"/>
        <SeekBar android:id="@+id/fmmediacontroller_progress" android:paddingLeft="10dp" android:paddingRight="10dp" android:paddingBottom="1dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="11dp" android:layout_toLeftOf="@+id/pause" android:layout_below="@+id/time_current" android:layout_centerVertical="true"/>
        <TextView android:textSize="12dp" android:textColor="#ffffffff" android:id="@+id/time" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="21dp" android:layout_marginRight="10dp" android:layout_alignRight="@+id/fmmediacontroller_progress"/>
    </RelativeLayout>
</RelativeLayout>
