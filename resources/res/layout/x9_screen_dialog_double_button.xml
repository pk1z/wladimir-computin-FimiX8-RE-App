            <TextView android:textSize="12.6dp" android:textColor="#cc3a3a3a" android:gravity="center" android:id="@+id/msg_tv" android:layout_width="251.6dp" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_marginTop="18dp" android:layout_marginRight="20dp" android:layout_marginBottom="39.3dp" android:layout_below="@+id/tv_title"/>
