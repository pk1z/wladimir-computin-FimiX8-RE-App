        </RelativeLayout>
    </LinearLayout>
    <View android:id="@+id/v_divider" android:background="@color/white_10" android:layout_width="1dp" android:layout_height="match_parent" android:layout_toRightOf="@+id/ll_fc_setting_items"/>
    <ViewStub android:id="@+id/stub_fc_item" android:layout="@layout/x8_main_fc_item" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_toRightOf="@+id/v_divider"/>
    <ViewStub android:id="@+id/stub_rc_item" android:layout="@layout/x8_main_rc_item" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_toRightOf="@+id/v_divider"/>
    <ViewStub android:id="@+id/stub_gimbal_item" android:layout="@layout/x8_main_gimbal_item" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_toRightOf="@+id/v_divider"/>
    <ViewStub android:id="@+id/stub_battery_item" android:layout="@layout/x8_main_battery_item" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_toRightOf="@+id/v_divider"/>
